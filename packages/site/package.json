{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "license": "(MIT-0 OR Apache-2.0)",
  "scripts": {
    "build": "cross-env GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "build-storybook": "build-storybook",
    "clean": "rimraf public",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "prettier:write": "prettier -w .",
    "start": "cross-env GATSBY_TELEMETRY_DISABLED=1 gatsby develop",
    "storybook": "start-storybook -p 6006",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      "chrome >= 67",
      "edge >= 79",
      "firefox >= 68",
      "opera >= 54",
      "safari >= 14"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.12.4",
    "@metamask/providers": "^9.0.0",
    "js-big-decimal": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "react-minimal-pie-chart": "^8.4.0",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.21.3",
    "@mdx-js/react": "^1.6.22",
    "@metamask/eslint-config": "^10.0.0",
    "@metamask/eslint-config-jest": "^10.0.0",
    "@metamask/eslint-config-nodejs": "^10.0.0",
    "@metamask/eslint-config-typescript": "^10.0.0",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-styling": "^0.3.2",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@svgr/webpack": "^6.4.0",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-loader": "^8.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-jsdoc": "^39.2.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "gatsby": "^4.24.4",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-styled-components": "^5.24.0",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "jest": "^29.5.0",
    "prettier": "^2.2.1",
    "prettier-plugin-packagejson": "^2.2.18",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.60.0",
    "sass-loader": "^13.2.2",
    "ts-jest": "^29.0.5",
    "typescript": "^4.7.4"
  }
}
